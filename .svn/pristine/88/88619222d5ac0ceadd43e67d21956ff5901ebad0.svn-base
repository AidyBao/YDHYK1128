#import "YDAddTimeCollectionViewCell.h"

@interface YDAddTimeCollectionViewCell()
@property (weak, nonatomic) IBOutlet UILabel *addTimeLabel;


@end

@implementation YDAddTimeCollectionViewCell

- (void)awakeFromNib {
    [super awakeFromNib];
    [self.layer setCornerRadius:16.0];
    [self.layer setMasksToBounds:YES];
    
    [self.layer setShadowOffset:CGSizeMake(0.0, 1.0)];
    [self.layer setShadowRadius:16.0];
    [self.layer setShadowColor:[UIColor blackColor].CGColor];
    [self.layer setShadowOpacity:0.5];
    
    [self.addTimeLabel setTextColor:[UIColor zx_sub2TextColor]];
    
}

@end
